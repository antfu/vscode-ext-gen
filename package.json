{
  "name": "vscode-ext-gen",
  "type": "module",
  "version": "0.3.6",
  "packageManager": "pnpm@9.7.1",
  "description": "Generate TypeScript meta info for VS Code extension from package.json",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/vscode-ext-gen#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/vscode-ext-gen.git"
  },
  "bugs": "https://github.com/antfu/vscode-ext-gen/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": "./bin.mjs",
  "files": [
    "bin.mjs",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks && git submodule update --init --recursive"
  },
  "publishConfig": {
    "scripts": {},
    "devDependencies": {}
  },
  "dependencies": {
    "cac": "^6.7.14",
    "scule": "^1.3.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.26.0",
    "@antfu/ni": "^0.22.4",
    "@antfu/utils": "^0.7.10",
    "@types/node": "^22.4.0",
    "@types/yargs": "^17.0.33",
    "bumpp": "^9.5.1",
    "eslint": "^9.9.0",
    "esno": "^4.7.0",
    "fast-glob": "^3.3.2",
    "lint-staged": "^15.2.9",
    "pnpm": "^9.7.1",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.16.6",
    "typescript": "^5.5.4",
    "unbuild": "^2.0.0",
    "vite": "^5.4.1",
    "vitest": "^2.0.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
