{
  "publisher": "calmripple",
  "name": "sample",
  "displayName": "sample IntelliSense",
  "type": "commonjs",
  "version": "0.8.1",
  "private": true,
  "packageManager": "pnpm@9.0.6",
  "description": "Intelligent sample previewing and searching for VS Code",
  "license": "MIT",
  "homepage": "https://github.com/calmripple/sample-extension",
  "repository": {
    "type": "git",
    "url": "https://github.com/calmripple/sample-extension"
  },
  "bugs": "https://github.com/calmripple/sample-extension/issues",
  "keywords": [
    "icons",
    "sample"
  ],
  "sponsor": {
    "url": "https://github.com/sponsors/calmripple"
  },
  "preview": true,
  "categories": [
    "Other",
    "Visualization",
    "Snippets"
  ],
  "main": "dist/index.js",
  "icon": "res/logo.png",
  "engines": {
    "vscode": "^1.81.0"
  },
  "activationEvents": [
    "workspaceContains:package.json",
    "onLanguage:vue",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:handlebars",
    "onLanguage:svelte",
    "onLanguage:html",
    "onLanguage:django-html",
    "onLanguage:css"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "html",
        "path": "snippets/html.json"
      },
      {
        "language": "vue",
        "path": "snippets/html.json"
      }
    ],
    "commands": [
      {
        "command": "sample.toggle-annotations",
        "category": "sample",
        "title": "Toggle Annotations"
      },
      {
        "command": "sample.toggle-inplace",
        "category": "sample",
        "title": "Toggle In-place Mode"
      },
      {
        "command": "sample.clear-cache",
        "category": "sample",
        "title": "Clear icon cache"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "sample IntelliSense",
      "properties": {
        "sample.inplace": {
          "type": "boolean",
          "default": true,
          "description": "Use icon graph to replace the icon name."
        },
        "sample.annotations": {
          "type": "boolean",
          "default": true,
          "description": "Enabled sample inline annotations"
        },
        "sample.position": {
          "type": "string",
          "enum": [
            "before",
            "after"
          ],
          "default": "before",
          "description": "Position the icon before or after the icon name"
        },
        "sample.color": {
          "type": "string",
          "default": "auto",
          "description": "Icon color hex for inline displaying"
        },
        "sample.delimiters": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            ":",
            "--",
            "-",
            "/"
          ],
          "description": "Delimiters for separating between collection id and icon id"
        },
        "sample.prefixes": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "",
            "i-",
            "~icons/"
          ],
          "description": "Prefixes for matching"
        },
        "sample.suffixes": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "",
            "i-"
          ],
          "description": "Suffixes for matching"
        },
        "sample.languageIds": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "javascript",
            "javascriptreact",
            "typescript",
            "typescriptreact",
            "vue",
            "svelte",
            "html",
            "pug",
            "json",
            "yaml"
          ],
